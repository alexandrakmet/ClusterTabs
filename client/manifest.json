{
  "name": "ClusterTabs",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png",
    "default_title": "ClusterTabs",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js",
        "js/content.js"
      ]
    }
  ],
  "permissions": [
    "tabGroups",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}